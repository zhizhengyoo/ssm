<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ynu.mapper.InventoryMapper">
    <resultMap id="BaseResultMap" type="com.ynu.dto.Inventory" >
        <id column="INVENTORY_ID" property="inventoryId" jdbcType="DECIMAL" />
        <result column="BOOK_ID" property="bookId" jdbcType="DECIMAL" />
        <result column="TOTAL_NUM" property="totalNum" jdbcType="DECIMAL" />
        <result column="SOLD_NUM" property="soldNum" jdbcType="DECIMAL" />
        <result column="REMAIN_NUM" property="remainNum" jdbcType="DECIMAL" />
    </resultMap>


    <sql id="Base_Column_List">
        INVENTORY_ID,BOOK_ID,TOTAL_NUM,SOLD_NUM,REMAIN_NUM
    </sql>


    <insert id="insertInventory">
      INSERT INTO
      sys_inventory(BOOK_ID,TOTAL_NUM,REMAIN_NUM)
      VALUES (#{bookId},#{totalNum},#{remainNum})
    </insert>



</mapper>